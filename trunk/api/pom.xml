                 <id>snapshots</id>
                 <url>http://ubuntu:8081/artifactory/repo</url>
                 <releases>
                         <enabled>false</enabled>
                 </releases>
         </pluginRepository>
 </pluginRepositories>
  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
